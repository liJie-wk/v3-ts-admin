{
  "rewrites": [
    {
      "source": "/prod_api/(.*)",
      "destination": "/api/proxy"
    },
    {
      "source": "/img_api/(.*)",
      "destination": "/api/proxy"
    },
    {
      "source": "/img2_api/(.*)",
      "destination": "/api/proxy"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}